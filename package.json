{
	"name": "magic_stream",
	"version": "1.0.0",
	"description": "Magic Stream Server, Web & App",
	"scripts": {
		"build-watch": "parcel watch ./web/index.html",
		"start-watch": "nodemon server/index.js",
		"dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
		
		"build": "parcel build ./web/index.html",
		"start": "npm run build && node server/index.js"
	},
	"dependencies": {
		"fastify": "^3.9.1",
		"fastify-https-redirect": "^1.0.3",
		"fastify-static": "^3.3.0",
		"fastify-websocket": "^2.0.11",
		"middie": "^5.2.0"
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"nodemon": "^2.0.6",
		"parcel": "^1.12.4",
		"parcel-bundler": "^1.12.4",
		"choo": "^7.1.0",
		"sass": "^1.30.0"
	},
	"author": "Marin Bivol",
	"license": "ISC"
}
